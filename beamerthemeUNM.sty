\newcommand{\coursenode}[3]{%
  \draw (#1.0) -- (#1.180);
  \node at ($ (#1) + (90:0.3cm)$) {$#2$};
  \node at ($ (#1) + (270:0.3cm)$) {$#3$};
}

\newcommand{\reqnode}[5]{%
  \draw (#1.0) -- (#1.180);
  \draw (#1.270) -- (#1.90);
  \node at ( $ (#1) + (135:0.3cm)$ ) {$#2$};
  \node at ( $ (#1) + (45:0.3cm)$ ) {$#3$};
  \node at ( $ (#1) + (225:0.3cm)$ ) {$#4$};
  \node at ( $ (#1) + (315:0.3cm)$ ) {$#5$};
}
